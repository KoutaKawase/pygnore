#!/home/suhrkawase/work/python/pygnore/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pygnore','console_scripts','pygnore'
__requires__ = 'pygnore'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pygnore', 'console_scripts', 'pygnore')()
    )
